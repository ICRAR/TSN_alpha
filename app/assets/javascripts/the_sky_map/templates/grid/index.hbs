x_min: {{x_min}}
x_max: {{x_max}}
<br />
y_min: {{y_min}}
y_max: {{y_max}}
<br />
z_min: {{z_min}}
z_max: {{z_max}}
<br />
<button {{action 'scroll_x_p'}}>scroll_x_p</button>
<button {{action 'scroll_x_n'}}>scroll_x_n</button>
<br />
<button {{action 'scroll_y_p'}}>scroll_y_p</button>
<button {{action 'scroll_y_n'}}>scroll_y_n</button>
<br />
<button {{action 'scroll_z_p'}}>scroll_z_p</button>
<button {{action 'scroll_z_n'}}>scroll_z_n</button>
<ul>


  {{#each grids_layers}}
      <li>
        Layer: {{z}}
          <ul>
              {{#each rows}}
                <li>
                    Row: {{y}}
                    <ul>
                        {{#each grids}}
                          <li>{{name}}::{{id}}</li>
                        {{/each}}
                    </ul>
                </li>
              {{/each}}
          </ul>
      </li>
  {{/each}}
</ul>